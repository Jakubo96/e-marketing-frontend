{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "app/*": [
        "app/*"
      ],
      "shared/*": [
        "app/shared/*"
      ],
      "core/*": [
        "app/core/*"
      ],
      "modules/*": [
        "app/modules/*"
      ],
      "@generated": [
        "app/generated"
      ],
      "environments/*": [
        "environments/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
